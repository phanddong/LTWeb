@model IEnumerable<PDD.SachOnline.Models.ReportInfo>

@{
    ViewBag.Title = "ThongKe";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<h2>THỐNG KÊ SỐ MÔ HÌNH THEO ANIME</h2>
<table border="1">
    <tr style="background-color:burlywood; font-weight:bold">
        <td>Mã CD</td>
        <td>Số mô hình</td>
        <td>Tổng số mô hình</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Count</td>
            <td>@item.Sum</td>
        </tr>
     }
</table>

